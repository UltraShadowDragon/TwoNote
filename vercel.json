{
    "version": 2,
    "builds": [
      {
        "src": "dist/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/login",
        "dest": "/dist/login/index.html"
      },
      {
        "src": "/notebook",
        "dest": "/dist/notebook/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      }
    ]
}
