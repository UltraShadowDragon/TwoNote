{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/login",
        "dest": "/login/index.html"
      },
      {
        "src": "/about",
        "dest": "/about/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  